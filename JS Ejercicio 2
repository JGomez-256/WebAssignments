//ejercicio#2 

// Pedir al usuario que ingrese los números separados por espacios
let entrada = prompt("Ingrese números separados por espacios:");

// Separar la entrada en una lista y convertir cada elemento a número entero
let numeros = entrada.split(" ").map(Number);

// Obtener el menor número
let menor = Math.min(...numeros);

// Mostrar el resultado
console.log(El menor número ingresado es: ${menor});
